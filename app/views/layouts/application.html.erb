  <%= csrf_meta_tags %>
</head>
<body class="<%= controller.controller_name %>">
<div class="container">
  <h2>Welcome to Omnibrel</h2>
  <div id="menu_wrapper">
    <ul id="mbmcpebul_table" class="mbmcpebul_menulist css_menu">
      <li><div class="arrow buttonbg"><a href="<%= landing_index_url %>">Home</a></div></li>
    </ul>
    <ul id="mbmcpebul_table" class="mbmcpebul_menulist css_menu">
      <li><div class="arrow buttonbg"><a href="<%= about_index_url %>">About</a></div>
        <ul>
          <li><a title="" href="<%= about_resources_url %>">Resources</a></li>
        </ul></li>
    </ul>
  </div>
  <p id="center">_________________________________________________________</p>
  <%= yield %>
</div>
<div class="footer">
  Created By: Ryan Bacheller
</div>
</body>
</html>
